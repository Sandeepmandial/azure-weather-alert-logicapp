                    }
                },
                "Parse_JSON": {
                    "runAfter": {
                        "HTTP": [
                            "Succeeded"
                        ]
                    },
                    "type": "ParseJson",
                    "inputs": {
                        "content": "@body('HTTP')",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "coord": {
                                    "type": "object",
                                    "properties": {
                                        "lon": {
                                            "type": "number"
                                        },
                                        "lat": {
                                            "type": "number"
                                        }
                                    }
                                },
                                "weather": {
